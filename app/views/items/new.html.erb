<h1>投稿新規登録</h1>
<%= form_with model:@item, url: items_path, method: :post, local:true do |f| %>
  <h4>投稿名</h4>
  <%= f.text_field :name %>
  <h4>投稿画像</h4>
  <%= f.attachment_field :image %>
  <h4>コメント</h4>
  <%= f.text_area :introduction %>
  <h4>タグ付け</h4>
  <%= f.text_area :tag %>
  <%= @tags %>
  <h4>金額を入力する</h4>
  <%= f.text_field :price %>

  <%= f.submit '投稿', class:"btn btn-success" %>
<% end %>