<h1>注文履歴一覧</h1>
<table class="table table-inverse">
  <thead>
   <tr>
     <th>注文日</th>
     <th>配送先</th>
     <th>注文商品</th>
     <th>支払金額</th>
     <th>注文詳細</th>
   </tr>
  </thead>
  <tbody>
    <% @orders.each do |order|%>
      <tr>
        <td>
          <%= order.created_at.strftime('%Y/%m/%d %H:%M:%S') %>
        </td>
        <td>
          <%= order.postal_code %>
          <%= order.address %>
          <%= order.name%>
        </td>
        <td>
          <%= order.item.name %>
        </td>
        <td>
          <%= order.item.price %>
        </td>
        <td>
         <%#= link_to "表示する", users_orders_path(user_id:@user.id), class:"btn btn-primary" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>