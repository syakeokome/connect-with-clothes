<div class="title">
    <h3 class="fas fa-crown nav-link ">
        <span class="font">DM History</span>
    </h3>
</div>
<div class="container">
  <table>
    <tbody>
      <% @users.each do |user| %>
      　<tr>
      　  <td>
        <%= attachment_image_tag user, :user_image,format:'jpeg', fallback:"no_image.jpg",:size=>'100x100' %>
        <%= link_to user_path(user) do %>
          <%= user.nickname %>
        <% end %>
         </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>